e`]\n\n// Get size of window\nconst windowWidth = gridPortalBot.tags.pixelWidth\nconst windowHeight = gridPortalBot.tags.pixelHeight\n\n// Determine size of tag portal\nlet tagPortalHeight\nlet tagPortalWidth\nswitch (size) {\n    case 'large':\n    tagPortalHeight = 360\n        tagPortalWidth = clamp(windowWidth * 0.4, 500, 1000)\n        if (tagPortalWidth > windowWidth * 0.9) tagPortalWidth = windowWidth - 10\n        break\n\n    default:\n        tagPortalHeight = 210\n        tagPortalWidth = clamp(windowWidth * 0.2, 350, 600)\n}\n\n// Keep from going off top or bottom\nvar top = clamp(gridPortalBot.tags.pointerPixelY - tagPortalHeight, 5, windowHeight - tagPortalHeight - 5)\n// Keep from going off left or right\nvar left = clamp(\n    gridPortalBot.tags.pointerPixelX - (tagPortalWidth / 2), // Number\n    5, // Min\n    windowWidth - tagPortalWidth - 5, // Max\n)\n\n// Update style object\nportalStyle.top  = `${ top }px`\nportalStyle.left = `${ left }px`\n\n// Update tag portal style\ntagPortalBot.tags.tagPortalStyle = `🧬${JSON.stringify(portalStyle)}`\n","smallPortalStyle":"🧬{\n    \"top\": \"10%\",\n    \"left\": \"10%\",\n    \"width\": \"clamp(350px, 20vw, 600px)\",\n    \"max-width\": \"calc(100vw - 10px)\",\n    \"height\": \"180px\",\n    \"box-shadow\": \"2px 5px 19px -4px rgba(0,0,0,0.48)\",\n    \"border-radius\": \"1rem\",\n    \"border\": \"4px solid #f2f2f2\"\n}","system":"tutorial.tagPortalManager","abIDStudio":"62435408-dd5d-4856-a29c-ef4eb04f7fee","abVersion":"10.10"}},"f56d8f0e-fbf2-4944-978d-378e329a8b4e":{"id":"f56d8f0e-fbf2-4944-978d-378e329a8b4e","space":"shared","tags":{"casualTutorial":true,"color":"#485056","mobileArrows":true,"mobileArrowsX":0,"mobileArrowsY":0,"pointable":"false","scale":"0.5","scaleX":"3.25","scaleY":"1.25","scaleZ":"0.25","abIDStudio":"62435408-dd5d-4856-a29c-ef4eb04f7fee","abVersion":"10.10"}},"f6b8046a-80c5-40ef-8ae2-7b3e3cf945a0":{"id":"f6b8046a-80c5-40ef-8ae2-7b3e3cf945a0","space":"shared","tags":{"casualTutor