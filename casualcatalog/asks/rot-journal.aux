Update(masks.items);\r\n}\r\n\r\n/*if(that.tags.includes(\"items\")){\r\n    let collectionsMenu = getBot(byTag(\"name\", \"collectionsMenu\"));\r\n    collectionsMenu.masks.collectionsCompletion[masks.openCollectionID] = masks.items.filter(x => x === 2).length;\r\n    collectionsMenu.masks.collectionsCompletion = collectionsMenu.masks.collectionsCompletion;\r\n}*/\r\n\r\nif (typeof thisBot.vars.onCompletionUpdate === \"function\"){\r\n    if (masks.openCollectionCompletion != null) thisBot.vars.onCompletionUpdate(masks.openCollectionCompletion);\r\n}\r\nconsole.log(\"collection1\")","onCreate":"@thisBot.onInstStreaming();","openApp":"@os.unregisterApp(\"collectionApp\");\nos.registerApp(\"collectionApp\", thisBot);\n\nconst App = thisBot.getApp();\n\n// spawns the collection of artifacts that we clicked on\n/*\nlet artifactSpawner = getBot(\"#system\", \"rot.artifactSpawner\")\nartifactSpawner.spawnArtifactGroup({\n    artifactVisualData: tags.artifactInfo,\n    artifactLocationData: tags.artifactLocationData\n})\n*/\nos.compileApp(\"collectionApp\", <App />)","system":"rot.app.collection1","rot-journal":"true","abVersion":"10.24"}},"c6a8f887-3326-4293-bdd3-797f0829a189":{"id":"c6a8f887-3326-4293-bdd3-797f0829a189","space":"shared","tags":{"creator":"34c3c210-5bf1-49cf-b151-ee2d07f0e673","abIDOrigin":"rot-journal","cursor":"pointer","getIsLoaded":"@masks.isLoaded = that;","label":"load inst","loadNoInput":"@const inst = that[0];\r\n\r\nasync function loadInst(instToLoad) {\r\n    return new Promise((resolve) => {\r\n\r\n        const onInstJoined = (listenerThat) => {\r\n            if (listenerThat.inst === instToLoad) {\r\n                removeListenTagListener(null, 'onInstJoined', onInstJoined);\r\n\r\n                // HACK: As of writing (Feb 1 2024) there is no way to know when ab has been fully loaded into the new inst.\r\n                // So until then, we are just going to wait a healthy amount of time and hope that ab is loaded by then.\r\n                os.sleep(2000).then(() => {\r\n                    resolve();\r\n                });\r\n            }\r\n        }\r\n\r\n        addListenTagListener(null, 'onInstJoined', onInstJoined)\r\n        os.loadInst(instToLoad);\r\n    })\r\n}\r\n\r\nif (inst) {\r\n    let pattern = that[1];\r\n\r\n    if (!pattern) {\r\n        pattern = null;\r\n    }\r\n\r\n    // Remove ab/pattern from URL query params if it is there. \r\n    // Otherwise the new ab in the new inst will use it to load an undesirable pattern.\r\n    configBot.tags.ab = null;\r\n    configBot.tags.pattern = null;\r\n\r\n    let loadMessage = 'Load inst ' + inst;\r\n    if (pattern) {\r\n        loadMessage += ' with pattern ' + pattern;\r\n    }\r\n\r\n    os.toast(loadMessage, 3);\r\n\r\n    await loadInst(inst);\r\n    \r\n    superShout(\"sendIsLoaded\");\r\n    await os.sleep(50);\r\n    \r\n    // Disable listening on the ab search bot for a moment.\r\n    abSearch.tags.listening = false;\r\n\r\n    // Super shout the ab patten loading function.\r\n    if (masks.isLoaded != true) superShout('onLookupABEggs', {\r\n        abID: pattern,\r\n        autoHatch: true,\r\n    });\r\n\r\n    //superShout(\"openMiniGrid\");\r\n\r\n    masks.isLoaded = false;\r\n    \r\n    // Wait for a moment and then turn listening on the ab search bot back on.\r\n    await os.sleep(1000);\r\n    abSearch.tags.listening = true;\r\n}","name":"sideLoadInst","onClick":"@const inst = await os.showInput(null, { title: 'Inst'});\n\nasync function loadInst(instToLoad) {\n    return new Promise((resolve) => {\n\n        const onInstJoined = (listenerThat) => {\n            if (listenerThat.inst === instToLoad) {\n                removeListenTagListener(null, 'onInstJoined', onInstJoined);\n\n                // HACK: As of writing (Feb 1 2024) there is no way to know when ab has been fully loaded into the new inst.\n                // So until then, we are just going to wait a healthy amount of time and hope that ab is loaded by then.\n                os.sleep(2000).then(() => {\n                    resolve();\n                });\n            }\n        }\n\n        addListenTagListener(null, 'onInstJoined', onInstJoined)\n        os.loadInst(instToLoad);\n    })\n}\n\nif (inst) {\n    let pattern = await os.showInput(null, { title: 'Pattern to load'});\n\n    if (!pattern) {\n        pattern = null;\n    }\n\n    // Remove ab/pattern from URL query params if it is there. \n    // Otherwise the new ab in the new inst will use it to load an undesirable pattern.\n    configBot.tags.ab = null;\n    configBot.tags.pattern = null;\n\n    let loadMessage = 'Load inst ' + inst;\n    if (pattern) {\n        loadMessage += ' with pattern ' + pattern;\n    }\n\n    os.toast(loadMessage, 3);\n\n    await loadInst(inst);\n\n    // Disable listening on the ab search bot for a moment.\n    abSearch.tags.listening = false;\n\n    // Super shout the ab patten loading function.\n    superShout('onLookupABEggs', {\n        abID: pattern,\n        autoHatch: true,\n    });\n\n    superShout(\"openMiniGrid\");\n    \n    // Wait for a moment and then turn listening on the ab search bot back on.\n    await os.sleep(1000);\n    abSearch.tags.listening = true;\n}","scaleX":"4","scaleY":"2","scaleZ":"0.3","sideLoad":"false","sideLoadX":"0","sideLoadY":"2","system":"rc-sideLoadInstTest.loadButton","rot-journal":"true","abVersion":"10.24"}},"cd08f7b4-96f8-4ad7-8047-3344632a65d6":{"id":"cd08f7b4-96f8-4ad7-8047-3344632a65d6","space":"shared","tags":{"creator":"34c3c210-5bf1-49cf-b151-ee2d07f0e673","abIDOrigin":"rot-journal","artifactData":[{"id":1,"attributes":{"Name":"Drum and Drumstick","Description":"Lake sturgeon are often represented in Native American art as a culturally significant creature. This drum and drumstick, made by artist Daniel LaBlance Bissell of the Fish Clan - Sault Ste. Marie Tribe of Chippewa Indians, depicts the iconic fish. Once found in great abundance, lake sturgeon, the largest and oldest of the Great Lakes, have almost faded into obscurity due to overfishing, particularly for oil and caviar products. Now considered a threatened species in Michigan, they are the focus of rehabilitation efforts by scientists and also regional communities, such as Native American tribes living along Michigan's waterways. Lake sturgeon play a role in the creation and migration stories of Native Americans, ceremonial cycles and structure of some communities. People relied on the river and the fish for subsistence and it is currently part of art, storytelling, song and ceremony. For Native American tribes in the region, the important cultural connection to this sacred place and fish continues today.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/d361aebbfe6f31cbd20d7599a12e722d744c12d82f242be53321b41cba8ace51.png","Year":"2015","createdAt":"2024-08-05T16:06:41.854Z","updatedAt":"2024-08-28T19:39:57.437Z","publishedAt":"2024-08-05T16:12:27.435Z","Interactive":false,"GRPMUrl":"https://www.grpmcollections.org/Detail/objects/174971","RealImageUrl":"https://www.grpmcollections.org/media/collectiveaccess/images/5/1/9/7352_ca_object_representations_media_51933_large.jpg","InteractiveUrl":null}},{"id":2,"attributes":{"Name":"Barrel","Description":"This barrel is made of wood.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/99c7d93c3d066259976eae916babc70552893a4cf6f6c80ed9b7ba535a291a91.png","Year":null,"createdAt":"2024-08-05T16:07:17.934Z","updatedAt":"2024-10-17T18:08:37.369Z","publishedAt":"2024-08-05T16:10:02.723Z","Interactive":true,"GRPMUrl":"https://grpmcollections.org/Detail/objects/55186","RealImageUrl":"https://grpmcollections.org/media/collectiveaccess/images/1/5/8/6/88317_ca_object_representations_media_158638_large.jpg","InteractiveUrl":"https://brandplayer.com/?pattern=collection1-Barrel&studio=231ad722-0bc6-44e9-a7ed-ff6ba7f119b4&bios=free%20inst"}},{"id":3,"attributes":{"Name":"Sturgeon Scute","Description":"Scutes refer to the hard  structures that cover the exterior of an animal (turtle, crocodile etc.).  It generally refers to a thickened horn or bony plate.   These Scutes appear to be from a sturgeon species.    Lake Sturgeon contain five rows of Scutes.   They are the primary defense for sturgeon as most of their body is cartilage.  Scutes and dermal plates are some of the only remnants of sturgeon that are found in archaeological sites. This scute was found in the Grand Rapids Public Museum's collections in 2016 and is likely from a Lake Sturgeon, possibly even one from the Grand River.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/d2ca0cafacf1795874987ad2205e9f378f6c982455b04a4c770eb97c3093deec.png","Year":null,"createdAt":"2024-08-05T16:20:20.185Z","updatedAt":"2024-08-28T19:40:36.306Z","publishedAt":"2024-08-05T16:23:33.810Z","Interactive":false,"GRPMUrl":"https://www.grpmcollections.org/Detail/objects/174892","RealImageUrl":"https://www.grpmcollections.org/media/collectiveaccess/images/1/8/1/6/94680_ca_object_representations_media_181635_large.jpg","InteractiveUrl":null}},{"id":4,"attributes":{"Name":"Trowel","Description":"To Do","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/534934f21f53bd9f8168abbefdd5719d5e292b253a645a88f506c2f646804826.png","Year":null,"createdAt":"2024-08-05T16:20:38.828Z","updatedAt":"2024-09-11T19:21:58.719Z","publishedAt":"2024-08-05T16:23:37.396Z","Interactive":true,"GRPMUrl":null,"RealImageUrl":null,"InteractiveUrl":"https://brandplayer.com/?pattern=collection1-Trowel&studio=231ad722-0bc6-44e9-a7ed-ff6ba7f119b4&bios=local%20inst"}},{"id":5,"attributes":{"Name":"Beer Bottle","Description":"This beer bottle is made of thick glass and has a cork with a metal fastener. It is embossed with the words \"Frey Bros | Grand Rapids | Mich.\". It is sealed and contains some remnants of its original contents, likely a lager-style beer.\n\nThis beer bottle was found underground while installing a sprinkler system near the intersection of Fulton and Diamond in Grand Rapids. It was produced by one of Grand Rapids' many local breweries in the late nineteenth century, the Frey Brothers, after the Civil War. Grand Rapids has a long history of beer production which has been revived with the explosion of craft breweries in the twenty-first century.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/8e6d1bf0428776c552daad9c8c68e09d899a954d2ce70c7886761728676779d0.png","Year":"1872 – 1892","createdAt":"2024-08-05T16:20:46.154Z","updatedAt":"2024-08-28T19:41:11.445Z","publishedAt":"2024-08-05T16:23:40.022Z","Interactive":false,"GRPMUrl":"https://grpmcollections.org/Detail/objects/176261","RealImageUrl":"https://grpmcollections.org/media/collectiveaccess/images/1/8/1/6/71127_ca_object_representations_media_181634_large.jpg","InteractiveUrl":null}},{"id":6,"attributes":{"Name":"Corn Planter","Description":"Hand-held corn planters such as this occupy an important place between ancient manual methods of planting and the modern mechanized agriculture of today. Between the mid-1800s and the early 1900s numerous designs for hand-held corn planters, also known as \"bill picks\" or \"corn jabbers\" were developed. The idea was to make planting corn seeds as simple, efficient, and regular as possible. This one-handed device made a small hole in the soil and deposited a seed in one motion. With a corn planter like this, a farmer could plant 2-4 acres a day, instead of only 1 acre using traditional tools.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/b54cbd2adeabbc069b0d8a4e5782caa94a6f04f21f6e2fe46dc91b7211005e61.png","Year":"circa 1900","createdAt":"2024-08-05T16:20:53.332Z","updatedAt":"2024-08-28T19:41:26.894Z","publishedAt":"2024-08-05T16:25:12.187Z","Interactive":false,"GRPMUrl":"https://www.grpmcollections.org/Detail/objects/44304","RealImageUrl":"https://www.grpmcollections.org/media/collectiveaccess/images/1/8/1/6/46611_ca_object_representations_media_181629_large.jpg","InteractiveUrl":null}},{"id":7,"attributes":{"Name":"Midland Painted Turtle (mount)","Description":null,"PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/82e6ffb7c5e77d0bd5e719f71684adfc439df604d44bf520ca930ec0331af1f0.png","Year":null,"createdAt":"2024-08-05T16:20:58.465Z","updatedAt":"2024-08-28T19:41:47.433Z","publishedAt":"2024-08-05T16:25:30.443Z","Interactive":false,"GRPMUrl":"https://grpmcollections.org/Detail/objects/97957","RealImageUrl":"https://grpmcollections.org/media/collectiveaccess/images/1/7/3/6/9069_ca_object_representations_media_173612_large.jpg","InteractiveUrl":null}},{"id":8,"attributes":{"Name":"Log-Marking Hammer","Description":"This log-marking hammer has a wood handle and a cast iron head with the letters AF in relief. Logging was a popular trade in the 19th century and in order to tell which logs belonged to which company, each one would be marked with a hammer such as this to prove ownership. Each company's hammers featured unique letters or designs to help distinguish them from others. ","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/e07935a308932ef0271e0a7fcf81a0e99d3109a45431636070d60e3c8834ac68.png","Year":null,"createdAt":"2024-08-05T16:21:05.206Z","updatedAt":"2024-08-28T19:42:23.631Z","publishedAt":"2024-08-05T16:25:48.827Z","Interactive":false,"GRPMUrl":"https://grpmcollections.org/Detail/objects/193403","RealImageUrl":"https://grpmcollections.org/media/collectiveaccess/images/1/5/1/0/31211_ca_object_representations_media_151069_large.jpg","InteractiveUrl":null}},{"id":9,"attributes":{"Name":"Limestone","Description":"Limestone is a sedimentary rock that often forms in warm, shallow marine water. It is made of mostly calcium carbonate and can be white, gray, or pink in color. This specimen is gray and has been cut into a slab, likely to be used in the construction of a limestone building.","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/ae436726c061d102411846bec019db3c8d06ccf7cea2241485ead70a581b73d5.png","Year":null,"createdAt":"2024-08-05T16:21:10.791Z","updatedAt":"2024-09-11T19:27:36.692Z","publishedAt":"2024-08-05T16:26:15.312Z","Interactive":true,"GRPMUrl":"https://grpmcollections.org/Detail/objects/184686","RealImageUrl":"https://grpmcollections.org/media/collectiveaccess/images/1/3/1/0/9934_ca_object_representations_media_131029_large.jpg","InteractiveUrl":"https://brandplayer.com/?pattern=collection1-Limestone&studio=231ad722-0bc6-44e9-a7ed-ff6ba7f119b4&bios=free%20inst"}},{"id":11,"attributes":{"Name":"Artifact Test","Description":"Description","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/534934f21f53bd9f8168abbefdd5719d5e292b253a645a88f506c2f646804826.png","Year":null,"createdAt":"2024-10-01T16:42:15.456Z","updatedAt":"2024-10-01T22:47:15.802Z","publishedAt":"2024-10-01T16:51:07.823Z","Interactive":false,"GRPMUrl":null,"RealImageUrl":null,"InteractiveUrl":null}},{"id":12,"attributes":{"Name":"Artifact Test 2","Description":"Description","PhotoUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/82e6ffb7c5e77d0bd5e719f71684adfc439df604d44bf520ca930ec0331af1f0.png","Year":null,"createdAt":"2024-10-05T18:03:42.802Z","updatedAt":"2024-10-17T18:23:59.149Z","publishedAt":"2024-10-05T18:04:16.894Z","Interactive":false,"GRPMUrl":null,"RealImageUrl":"https://brandplayer-prod-filesbucket-458964701190.s3.amazonaws.com/f1bbae58-61d7-48d2-9ec9-47dbd463eca9/82e6ffb7c5e77d0bd5e719f71684adfc439df604d44bf520ca930ec0331af1f0.png","InteractiveUrl":null}}],"color":"brown","discoverableData":[{"id":6,"attributes":{"Name":"Limestone Church","Description":"A Gothic Revival style church building with two tall spires. The facade is primarily made of limestone with stone details. There is a large central entrance flanked by evenly placed windows. The building sits on a tree-lined street with small shrubs and a manicured lawn","Latitude":42.966461,"Longitude":-85.667559,"GRPMUrl":"https://grpmcollections.org/Detail/collections/514","createdAt":"2024-12-09T23:25:13.779Z","updatedAt":"2024-12-10T00:51:26.069Z","publishedAt":"2024-12-09T23:26:04.432Z"}},{"id":7,"attributes":{"Name":"Log Jam at the Blue Bridge","Description":"A vibrant blue steel truss bridge with multiple spans supported by sturdy stone pillars over a river.","Latitude":42.964706,"Longitude":-85.675429,"GRPMUrl":"https://grpmcollections.org/Detail/collections/511","createdAt":"2024-12-10T00:45:43.339Z","updatedAt":"2024-12-10T00:51:44.331Z","publishedAt":"2024-12-10T00:51:44.239Z"}},{"id":8,"attributes":{"Name":"Riverside Storytelling","Description":"A handcrafted Native American-style drum with a wooden frame and a taut, light-colored leather drumhead. The drumhead features a central circle design that resembles a crosshair pattern, surrounded by an intricate starburst or sunburst pattern, accented with small colored dots. There are also fish-shaped drum sticks.","Latitude":42.966396,"Longitude":-85.67607,"GRPMUrl":"https://grpmcollections.org/Detail/collections/512","createdAt":"2024-12-10T01:07:43.633Z","updatedAt":"2024-12-10T01:09:21.480Z","publishedAt":"2024-12-10T01:09:21.472Z"}},{"id":9,"attributes":{"Name":"The Stink Hole","Description":"A vintage, dark glass bottle with a long neck and a swing-top closure secured with wire. The bottle appears to have embossed or raised designs or text on its surface, possibly indicating a brand or logo. There is visible sedim