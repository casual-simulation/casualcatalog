n    const tempDropdown = {\r\n        ...menuOptions,\r\n        label: places[i].tags.bbLabel,\r\n        place: getLink(places[i]),\r\n        onClick:  `@\r\n            links.door.tags.destination = links.place.tags.simID;\r\n            links.door.tags.bbLabel = links.place.tags.bbLabel;\r\n            links.door.updateBillboardLabel();\r\n            shout(\"clearSimDoorMenu\");\r\n        `\r\n    }\r\n\r\n    options.push(tempDropdown);\r\n}\r\n\r\nchooseExitButton.dropdownOptions = options;\r\n\r\nconst travelButton = {\r\n    ...menuOptions,\r\n    label: 'travel to: ' + tags.bbLabel ?? '',\r\n    place: getLink(getBot(\"simID\", tags.destination)),\r\n    simDoor_menuSortOrder: 1,\r\n    onClick: `@\r\n        shout(\"activateStoryPlaceSkybox\", links?.place?.tags.formAddress);\r\n    `\r\n}\r\n\r\nif (!tags.destination) {\r\n    if (options.length == 0) {\r\n        os.toast(\"no destinations found\");\r\n    } else {\r\n    ab.links.menu.abCreateMenuDropdown(chooseExitButton); \r\n    }\r\n} else {\r\n    ab.links.menu.abCreateMenuButton(travelButton);\r\n}","abArtifactName":"simDoor","abIDOrigin":"simDoor","onBotAdded":"@let waitTime = 0;\r\n\r\nwhile(!globalThis.ab?.links.bot_factory) {\r\n    if (waitTime >= 5000) {\r\n        return;\r\n    }\r\n    \r\n    await os.sleep(250);\r\n    waitTime += 250;\r\n}\r\n\r\nmasks.ready = true;\r\n\r\nthisBot.updateBillboardLabel();","onBotChanged":"@const needUpdateBillboardLabel = that.tags.some(t => t === 'bbLabel' || t === 'bbLabelFloatingBackgroundColor' || t === 'bbLabelColor');\r\n\r\nif (needUpdateBillboardLabel && tags.ready) {\r\n    thisBot.updateBillboardLabel();\r\n}","updateBillboardLabel":"@if (thisBot.vars.billboardLabelBot) {\r\n    destroy(thisBot.vars.billboardLabelBot);\r\n    thisBot.vars.billboardLabelBot = null;\r\n}\r\n\r\nthisBot.vars.billboardLabelBot = ab.links.bot_factory.abCreateBillboardLabel({ \r\n    bot: thisBot, \r\n    label: tags.bbLabel,\r\n    color: tags.bbLabelFloatingBa